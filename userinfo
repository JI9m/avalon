const Discord = module.require("discord.js");
const fs = require("fs");
module.exports.run = async (bot,message,args) => {
    let rUser = message.guild.member(message.mentions.users.first() || message.guild.members.get(args[0])) || message.author
    
    let embed = new Discord.RichEmbed()
    .setDescription("Информация об участнике")
    .setColor('#ff0000')
    .addField("Имя",rUser.user.username)
    .addField("Тэг",rUser.user.tag)
    .addField("Присоединился к серверу",message.guild.joinedAt)
    .addField("Количество предупрежденией",rUser.guild.warns)
    .addField("Дата создания",rUser.guild.name)
    .setThumbnail(rUser.user.avatarURL)
    
    bot.send(embed);

};
module.exports.help = {
    name: "user"
};
